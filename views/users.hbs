<h1>{{title}}</h1>

<ul>
    {{#each users}}
    <li>Name: {{name}}; Age: {{age}}; Id: {{id}}</li>
    {{/each}}
</ul>

<form action="/add/user" method="post">
    <input type="text" placeholder="Name" name="name">
    <input type="number" placeholder="Age" name="age">

    <input type="submit">
</form>